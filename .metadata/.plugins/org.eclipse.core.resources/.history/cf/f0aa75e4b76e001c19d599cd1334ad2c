package level1;

import java.util.Scanner;

public class Test05 {

	public static void main(String[] args) {
		int A, B;
		
		Scanner scan = new Scanner(System.in);
		System.out.print("A=");
		A = scan.nextInt();
		System.out.println("B=");
		B = scan.nextInt();
		
		System.out.println(A + B);
		
	}

}
